package practice;

public class LastDay 
{

	/**
	 * always use this pattern before starting anycode
	 * String
	 * @param word
	 * @return
	 */
	public String returnSomething(String word) 
	{
		String result = "";
		
		return result;

	}
	
	/**
	 * always use this pattern before starting anycode
	 * Boolean
	 * @param word
	 * @return
	 */
	public boolean returnSomething1(String word) 
	{
		boolean result = false;
		
		return result;

	}
	
	/**
	 * always use this pattern before starting anycode
	 * loops
	 * @param word
	 * @return
	 */
	public boolean checkIfEmpty(String word) 
	{
		boolean result = false;
		if(word.isEmpty())
		{
			result = true;
		}
		
		return result;

	}

}
