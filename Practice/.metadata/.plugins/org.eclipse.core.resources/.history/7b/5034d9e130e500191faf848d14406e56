package practice;

/**
 * return true if the given string contains between 1 and 3 'e' chars.
 * 
 * stringE("Hello") true
 * stringE("Heelle") true
 * stringE("Heelele") false
 * @author tet19171871
 *
 */
public class LastDay1
{
	public boolean stringE(String str)
	{
		boolean greeting = false;
		
		if(str.length() == ('e'))
		{
			return true;
		}
		
		
		
		return greeting;
	}
	

}
