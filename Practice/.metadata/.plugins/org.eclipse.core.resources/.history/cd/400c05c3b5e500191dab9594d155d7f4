package practice;



/**
 * return an array of multiples of 5 between 0 and 100
 * @author tet19171871
 *
 */
public class Multiple
{
	public static void main(String[] args)
	{
		int i;
        for (i = 0; i <= 100; i++) 
        {
        	//checks if the remainder is 0 after dividing the index by 5
            if (i % 5 == 0) 
            {
            	//prints the index
                System.out.println(i);
		
            }
	
		}
	}
}

