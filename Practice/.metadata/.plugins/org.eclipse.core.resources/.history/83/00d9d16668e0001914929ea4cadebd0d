package practice;

public class MethodExample
{

	public static void main(String[] args)
	{
		MethodExample example = new MethodExample();
		//pass the value of the method signature in the method here
		example.sleepIn(10,true, false, false);

	}
	
	//pass the method signature to the value you want to be printed boolean or integer
	public void sleepIn(int integer, boolean tooSleepIn, boolean vacation, boolean weekend)
	{
		System.out.println("Sleep in: " + tooSleepIn);
		System.out.println("Vacation in: " + vacation);
		System.out.println("Weekend in: " + weekend);
		System.out.println("Integer in: " + integer);
	}

}

